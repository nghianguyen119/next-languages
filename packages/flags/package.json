{
  "name": "@next-languages/flags",
  "version": "0.0.0",
  "description": "A simple react component to display flags for different languages",
  "author": "github/nghianguyen119",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "homepage": "",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "default": "./dist/cjs/index.js"
      }
    },
    "./*.js": {
      "import": {
        "types": "./dist/esm/types/flags/*.d.ts",
        "default": "./dist/esm/flags/*.js"
      },
      "require": {
        "default": "./dist/cjs/flags/*.js"
      }
    },
    "./style.css": {
      "import": "./dist/esm/style.css",
      "require": "./dist/cjs/style.css"
    }
  },
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "echo 'Add dev script here'",
    "build": "rimraf dist && rollup --config",
    "test": "echo 'Add test script here'",
    "lint": "echo 'Add lint script here'",
    "post-process-svg": "tsx ./scripts/post-process-svg.ts",
    "svg-to-tsx": "svgr assets/flags --config-file .svgrrc"
  },
  "devDependencies": {
    "@next-languages/eslint-config": "workspace:*",
    "@next-languages/typescript-config": "workspace:*",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-url": "^8.0.2",
    "@svgr/cli": "^8.1.0",
    "@types/eslint": "^8.56.5",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "cheerio": "1.0.0-rc.12",
    "class-variance-authority": "^0.7.0",
    "clsx": "~2.1.0",
    "eslint": "^8.57.0",
    "node-sass": "^9.0.0",
    "postcss-scss": "^4.0.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.14.2",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.75.0",
    "tslib": "^2.6.2",
    "tsx": "^4.7.2",
    "typescript": "^5.3.3"
  }
}
