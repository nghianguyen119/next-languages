{
  "name": "@next-languages/flags",
  "version": "0.1.1",
  "description": "A simple react component to display flags for different languages",
  "author": "github/nghianguyen119",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nghianguyen119/next-languages"
  },
  "homepage": "https://github.com/nghianguyen119/next-languages",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "default": "./dist/cjs/index.js"
      }
    },
    "./*.js": {
      "import": {
        "types": "./dist/esm/types/components/flags/default/*.d.ts",
        "default": "./dist/esm/components/flags/default/*.js"
      },
      "require": {
        "default": "./dist/cjs/components/flags/default/*.js"
      }
    },
    "./style.css": {
      "import": "./dist/esm/style.css",
      "require": "./dist/cjs/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "rimraf dist && rollup --config",
    "generate": "tsx ./scripts/generate-flag-components.ts"
  },
  "peerDependencies": {
    "class-variance-authority": "^0.7.0",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@babel/traverse": "^7.24.1",
    "@next-languages/eslint-config": "workspace:*",
    "@next-languages/typescript-config": "workspace:*",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-url": "^8.0.2",
    "@svgr/cli": "^8.1.0",
    "@svgr/core": "^8.1.0",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.5",
    "@types/eslint": "^8.56.5",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@types/svg-parser": "^2.0.6",
    "cheerio": "1.0.0-rc.12",
    "class-variance-authority": "^0.7.0",
    "eslint": "^8.57.0",
    "node-sass": "^9.0.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-parse": "^9.0.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.14.2",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.75.0",
    "svg-parser": "^2.0.4",
    "tslib": "^2.6.2",
    "tsx": "^4.7.2",
    "typescript": "^5.3.3",
    "unified": "^11.0.4"
  },
  "dependencies": {
    "@babel/generator": "^7.24.4",
    "@babel/parser": "^7.24.4",
    "@babel/types": "^7.24.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0"
  }
}
